    <xsd:element name="isSubtaskResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="result" type="xsd:boolean"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="release">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="identifier" type="xsd:anyURI"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="releaseResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:annotation>
