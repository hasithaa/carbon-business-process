        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="identifier" type="xsd:anyURI"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="addComment">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="identifier" type="xsd:anyURI"/>
