    <xsd:element name="batchClaimResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="batchResponse" type="hta:tBatchResponse" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
