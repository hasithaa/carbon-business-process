            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="completeResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:annotation>
                    <xsd:documentation>Empty message</xsd:documentation>
                </xsd:annotation>
