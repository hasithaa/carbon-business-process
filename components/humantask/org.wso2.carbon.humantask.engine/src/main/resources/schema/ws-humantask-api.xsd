        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="parentTaskIdentifier" type="xsd:anyURI" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getRendering">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="identifier" type="xsd:anyURI"/>
                <xsd:element name="renderingType" type="xsd:QName"/>
            </xsd:sequence>
        </xsd:complexType>
