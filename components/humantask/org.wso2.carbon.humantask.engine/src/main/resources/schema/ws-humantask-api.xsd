        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="renderingType" type="xsd:QName" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="getSubtaskIdentifiers">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="taskIdentifier" type="xsd:anyURI"/>
            </xsd:sequence>
