    <xsd:element name="getTaskHistory">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="identifier" type="xsd:anyURI"/>
                <xsd:element name="filter" type="htt:tTaskHistoryFilter" minOccurs="0"/>
