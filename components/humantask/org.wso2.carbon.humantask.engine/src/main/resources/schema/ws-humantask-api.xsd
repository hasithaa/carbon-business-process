        <xsd:complexType>
            <xsd:sequence>
                <xsd:annotation>
                    <xsd:documentation>Empty message</xsd:documentation>
                </xsd:annotation>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

